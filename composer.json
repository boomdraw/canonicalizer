{
    "name": "boomdraw/canonicalizer",
    "description": "Canonicalizer package for Laravel",
    "keywords": [
        "boomdraw",
        "laravel",
        "canonicalizer",
        "canonicalization"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/boomdraw/canonicalizer",
    "authors": [
        {
            "name": "Ivan Slesarenko",
            "email": "info@boomdraw.com",
            "homepage": "https://boomdraw.com",
            "role": "Owner"
        }
    ],
    "support": {
        "email": "info@boomdraw.com"
    },
    "require": {
        "php": "^7.2",
        "illuminate/support": "~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Boomdraw\\Canonicalizer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Boomdraw\\Canonicalizer\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Boomdraw\\Canonicalizer\\CanonicalizerServiceProvider"
            ],
            "aliases": {
                "Canonicalizer": "Boomdraw\\Canonicalizer\\Facades\\Canonicalizer"
            }
        }
    }
}
